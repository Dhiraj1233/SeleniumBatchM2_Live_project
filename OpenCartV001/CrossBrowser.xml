<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="ParallelTesting" thread-count="5">
	<listeners>
	 <listener class-name="com.OpenCart.Utility.ListenersClass"></listener>
 </listeners>
 <test name="ChromeTest">
	 <parameter name="Os" value="windows"></parameter> 
	<parameter name="browser" value="Chrome"></parameter>
      <classes>
     	<class name="com.OpenCart.TestCases.TC01_Account_Registration"/> 
     	<class name="com.OpenCart.TestCases.TC02_LoginTest"></class>
       <class name="com.OpenCart.TestCases.TC02_LoginDdt"></class>
      </classes>
 </test>  
 <test name="FirefoxTest">
	 <parameter name="Os" value="windows"></parameter> 
    <parameter name="browser" value="Firefox"></parameter>
      <classes>
		<class name="com.OpenCart.TestCases.TC01_Account_Registration"/> 
     	<class name="com.OpenCart.TestCases.TC02_LoginTest"></class>
        <class name="com.OpenCart.TestCases.TC02_LoginDdt"></class>   
    </classes>
 </test>  

</suite>